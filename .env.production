# FISO Production Environment Configuration
# Copy this file and configure with your actual values

# API Configuration
REACT_APP_API_URL=https://your-production-api.com
REACT_APP_API_TOKEN=your-production-api-token
REACT_APP_AUTH_URL=https://your-auth-service.com

# Cloud Provider APIs (configure at least one)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_DEFAULT_REGION=us-east-1

AZURE_CLIENT_ID=your-azure-client-id
AZURE_CLIENT_SECRET=your-azure-client-secret
AZURE_TENANT_ID=your-azure-tenant-id

GCP_PROJECT_ID=your-gcp-project-id
GCP_SERVICE_ACCOUNT_PATH=path/to/service-account.json

# Database Configuration
DATABASE_URL=sqlite:///data/fiso_production.db
DATABASE_POOL_SIZE=10
DATABASE_TIMEOUT=30

# Security
SECRET_KEY=your-super-secret-key-here
JWT_SECRET=your-jwt-secret-key
ENCRYPTION_KEY=your-encryption-key

# Monitoring & Logging
LOG_LEVEL=INFO
ENABLE_MONITORING=true
PROMETHEUS_PORT=9090

# Feature Flags
ENABLE_REAL_DATA=true
ENABLE_ML_PREDICTIONS=true
ENABLE_COST_OPTIMIZATION=true